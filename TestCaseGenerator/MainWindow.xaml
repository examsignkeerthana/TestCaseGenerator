<Window x:Class="TestCaseGenerator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestCaseGenerator"
        mc:Ignorable="d"
        Title="MainWindow" Height="768" Width="1024">
    <Grid >
        <StackPanel Orientation="Horizontal">
            <Button VerticalAlignment="Top" Width="500" FontSize="24" Height="50" x:Name="btnSelectionFrame" Content="Course" Click="btnSelectionFrame_Click"/>
            <Button VerticalAlignment="Top" Width="500" FontSize="24" x:Name="btnQuesFrame" Content="Course" Click="btnQuesFrame_Click" Height="50"/>
        </StackPanel>
        <Frame x:Name="MainFrame" Width="1024" Height="668"></Frame>
    </Grid>
</Window>
